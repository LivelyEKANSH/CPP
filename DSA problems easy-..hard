//Take name as input and print a greeting message for that particular name.


#include <iostream>
using namespace std;


int main()
{
    int T;
    string name;
    cin>>T;
    while(T--)
    {
        cin>>name;
        cout<<"Good afternoon "<<name<<", it's a pleasure to meet you"<<endl;
    }
    return 0;
}








