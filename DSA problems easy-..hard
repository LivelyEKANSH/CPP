//Write a program to input principal, time, and rate (P, T, R) from the user and find Simple Interest.




#include <iostream>
using namespace std;


int main()
{
    int T,p,t;
    float r;
    cin>>T;
    while(T--)
    {
        cin>>p>>r>>t;
        cout<<(p*r*t)/2<<endl;
    }
    return 0;
}







